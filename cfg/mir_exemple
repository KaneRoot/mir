function mir_exemple()
{
	### var env Ã  modifier au besoin ###
	#	  source ; bande_passante	   #
	####################################

	/usr/bin/rsync -rqtlH --safe-links --delete-after \
		--delay-updates --bwlimit=${bande_passante} \
		--temp-dir="${tmp}" \
		--exclude='*.links.tar.gz*' \
		--exclude='/other' \
		--exclude='/sources' \
		--exclude='/iso' \
		${source} \
		"${target}"
}
